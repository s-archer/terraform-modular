{
  "policy": {
    "applicationLanguage": "utf-8",
    "blocking-settings": {
      "evasions": [
        {
          "description": "Bad unescape",
          "enabled": true,
          "learn": true
        },
        {
          "description": "Apache whitespace",
          "enabled": true,
          "learn": true
        },
        {
          "description": "Bare byte decoding",
          "enabled": true,
          "learn": true
        },
        {
          "description": "IIS Unicode codepoints",
          "enabled": true,
          "learn": true
        },
        {
          "description": "IIS backslashes",
          "enabled": true,
          "learn": true
        },
        {
          "description": "%u decoding",
          "enabled": true,
          "learn": true
        },
        {
          "description": "Multiple decoding",
          "enabled": true,
          "learn": true,
          "maxDecodingPasses": 3
        },
        {
          "description": "Directory traversals",
          "enabled": true,
          "learn": true
        }
      ],
      "violations": [
        {
          "alarm": true,
          "block": true,
          "description": "ASM Cookie Hijacking",
          "learn": false,
          "name": "VIOL_ASM_COOKIE_HIJACKING"
        },
        {
          "alarm": true,
          "block": true,
          "description": "Access from disallowed User/Session/IP/Device ID",
          "name": "VIOL_SESSION_AWARENESS"
        },
        {
          "alarm": true,
          "block": true,
          "description": "Modified ASM cookie",
          "learn": true,
          "name": "VIOL_ASM_COOKIE_MODIFIED"
        },
        {
          "alarm": true,
          "block": true,
          "learn": false,
          "name": "VIOL_LOGIN_URL_BYPASSED"
        },
        {
          "alarm": true,
          "block": true,
          "description": "XML data does not comply with format settings",
          "learn": true,
          "name": "VIOL_XML_FORMAT"
        },
        {
          "alarm": true,
          "block": true,
          "learn": true,
          "name": "VIOL_FILETYPE"
        },
        {
          "alarm": true,
          "block": true,
          "learn": true,
          "name": "VIOL_URL"
        },
        {
          "alarm": true,
          "block": true,
          "learn": true,
          "name": "VIOL_URL_METACHAR"
        },
        {
          "alarm": true,
          "block": true,
          "learn": true,
          "name": "VIOL_PARAMETER_VALUE_METACHAR"
        },
        {
          "alarm": true,
          "block": true,
          "learn": true,
          "name": "VIOL_PARAMETER_NAME_METACHAR"
        }
      ]
    },
    "brute-force-attack-preventions": [
      {
        "bruteForceProtectionForAllLoginPages": true,
        "leakedCredentialsCriteria": {
          "action": "alarm-and-blocking-page",
          "enabled": true
        }
      }
    ],
    "caseInsensitive": true,
    "csrf-protection": {
      "enabled": true
    },
    "csrf-urls": [
      {
        "enforcementAction": "verify-csrf-token",
        "method": "GET",
        "url": "/trading/index.php"
      }
    ],
    "data-guard": {
      "enabled": true
    },
    "description": "A basic, OWASP Top 10 protection items v1.0",
    "enforcementMode": "blocking",
    "general": {
      "trustXff": true
    },
    "json-profiles": [
      {
        "name": "portfolio"
      }
    ],
    "login-enforcement": {
      "authenticatedUrls": [
        "/trading/index.php"
      ]
    },
    "login-pages": [
      {
        "accessValidation": {
          "headerContains": "302 Found"
        },
        "authenticationType": "form",
        "passwordParameterName": "password",
        "url": {
          "method": "POST",
          "name": "/trading/auth.php",
          "protocol": "https",
          "type": "explicit"
        },
        "usernameParameterName": "username"
      }
    ],
    "name": "Complete_OWASP_Top_Ten",
    "policy-builder-server-technologies": {
      "enableServerTechnologiesDetection": true
    },
    "protocolIndependent": true,
    "session-tracking": {
      "sessionTrackingConfiguration": {
        "enableTrackingSessionHijackingByDeviceId": true
      }
    },
    "signature-settings": {
      "minimumAccuracyForAutoAddedSignatures": "high",
      "signatureStaging": false
    },
    "template": {
      "name": "POLICY_TEMPLATE_RAPID_DEPLOYMENT"
    },
    "urls": [
      {
        "method": "POST",
        "name": "/trading/auth.php",
        "protocol": "https",
        "type": "explicit"
      },
      {
        "isAllowed": false,
        "method": "GET",
        "name": "/internal/test.php",
        "protocol": "https",
        "type": "explicit"
      },
      {
        "method": "GET",
        "name": "/trading/rest/portfolio.php",
        "protocol": "https",
        "type": "explicit",
        "urlContentProfiles": [
          {
            "contentProfile": {
              "name": "portfolio"
            },
            "headerName": "Content-Type",
            "headerValue": "text/html",
            "type": "json"
          },
          {
            "headerName": "*",
            "headerValue": "*",
            "type": "do-nothing"
          }
        ]
      }
    ],
    "xml-profiles": [
      {
        "defenseAttributes": {
          "allowDTDs": false,
          "allowExternalReferences": false
        },
        "name": "Default"
      }
    ]
  }
}